</head>
<body class="vertical-layout vertical-compact-menu 2-columns   menu-expanded fixed-navbar" data-open="click" data-menu="vertical-compact-menu" data-col="2-columns">


    <!-- fixed-top-->
    <nav class="header-navbar navbar-expand-md navbar navbar-with-menu navbar-without-dd-arrow fixed-top navbar-light navbar-bg-color">
        <div class="navbar-wrapper">
            <div class="navbar-header d-md-none">
                <ul class="nav navbar-nav flex-row">
                    <li class="nav-item mobile-menu d-md-none mr-auto"><a class="nav-link nav-menu-main menu-toggle hidden-xs" href="#"><i class="ft-menu font-large-1"></i></a></li>
                    <li class="nav-item d-md-none"><a class="navbar-brand" href="index.html"><img class="brand-logo d-none d-md-block" alt="TALLENTEX admin logo" src="https://www.tallentex.com/assets/images/logo/tx-logo-white.png"><img class="brand-logo d-sm-block d-md-none" alt="TALLENTEX admin logo sm" src="https://www.tallentex.com/assets/images/logo/tx-logo-white.png"></a></li>
                    <li class="nav-item d-md-none"><a class="nav-link open-navbar-container" data-toggle="collapse" data-target="#navbar-mobile"><i class="la la-ellipsis-v">   </i></a></li>
                </ul>
            </div>
            <div class="navbar-container">
                <div class="collapse navbar-collapse" id="navbar-mobile" style="background:rgb(249, 250, 253);border: 1px solid #d8e2ef">
                    <ul class="nav navbar-nav mr-auto float-left">
                        <li class="nav-item d-none d-md-block"><a class="nav-link nav-menu-main menu-toggle hidden-xs" href="#"><i class="ft-menu">         </i></a></li>
                        <li class="nav-item d-none d-md-block"><img src="~/app-assets/images/backgrounds/crm-bar-chart.a1e2dfbe9869ca65c721.png" alt="..." class="ms-n4" style="height: 70px;"></li>
                        <li class="nav-item d-none d-md-block">
                            <a class="nav-link nav-menu-main menu-toggle hidden-xs">This is your TALLENTEX-@LNResources.Session Result & Elaborative Performance Analysis. ALLEN wishes you a bright career ahead!</a>
                        </li>
                        <li class="nav-item d-none d-md-block"><img src="~/app-assets/images/backgrounds/crm-line-chart.cee93078e2f6ab35de00.png" alt="..." class="ms-n4" style="height: 70px;"></li>
                    </ul>
                    <ul class="nav navbar-nav float-right">
                        <li class="dropdown dropdown-user nav-item">
                            <a class="dropdown-toggle nav-link dropdown-user-link">
                                <span class="mr-1">Welcome <span class="me-2" role="img" aria-label="Emoji">📢</span> @ViewBag.Name! (@ViewBag.Fno)</span>
                            </a>
                        </li>
                        <li class="dropdown dropdown-user nav-item">
                            <a class="dropdown-toggle nav-link dropdown-user-link" href="#" data-toggle="dropdown">
                                <span class="avatar avatar-online"><img src="@ViewBag.Photo" alt="avatar"></span>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right">
                                <a class="dropdown-item" href="~/result/dashboard/"><i class="ft-award"></i>@ViewBag.Name</a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" href="~/result/dashboard/"><i class="ft-user"></i> Profile</a>
                                <a class="dropdown-item" href="~/analysis/" target="_blank"><i class="icon-bar-chart"></i> Performance Analysis</a>
                                <a class="dropdown-item" href="~/ifa/"><i class="icon-book-open"></i> ALLEN Admission Info</a>
                                <div class="dropdown-divider"></div><a class="dropdown-item" href="javascript:logout();">
                                    <i class="ft-power"></i> Logout
                                </a>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>

    <!-- ////////////////////////////////////////////////////////////////////////////-->

    <div class="main-menu menu-fixed menu-dark menu-bg-default rounded menu-accordion menu-shadow">
        <div class="main-menu-content">
            <a class="navigation-brand d-none d-md-block d-lg-block d-xl-block" style="border-bottom:1px solid rgba(237, 242, 249, 0.1);" href="index.html"><img class="brand-logo" alt="TALLENTEX admin logo" src="https://www.tallentex.com/assets/images/logo/tx-logo-white.png" /></a>
            <ul class="navigation navigation-main mt-3" id="main-menu-navigation" data-menu="menu-navigation">
                <li class="active">
                    <a href="~/result/dashboard"><i class="icon-grid"></i><span class="menu-title" data-i18n="">Dashboard</span></a>
                </li>
                @if (ViewBag.ShowResult == "1")
                {
                    <li class=" nav-item">
                        <a href="~/analysis/" target="_blank"><i class="icon-bar-chart"></i><span class="menu-title" data-i18n="">Performance<br />Analysis</span></a>
                    </li>
                    <li class=" nav-item">
                        <a href="~/ifa/"><i class="icon-book-open"></i><span class="menu-title" data-i18n="">ALLEN<br />Admission<br />Info</span></a>
                    </li>
                }
                <li class=" nav-item">
                    <a href="https://www.tallentex.com/"><i class="icon-support"></i><span class="menu-title" data-i18n="">TALLENTEX<br />Home</span></a>
                </li>
                <li class="navigation-divider"></li>
                <li class=" nav-item">
                    <a href="javascript:logout();"><i class="ft-power"></i><span class="menu-title" data-i18n="">Logout</span></a>
                </li>
            </ul>
        </div>
    </div>

    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body">
                @if (ViewBag.DocUpload != null)
                {
                    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css">
                    <div class="row">
                        <div class="col-12 col-md-12">
                            @if (ViewBag.isSend == 1)
                            {
                                <div class="text-center alert alert-success">
                                    The verification process for your documents has been completed successfully, and your cash reward will be transferred shortly.
                                </div>
                            }
                            else
                            {
                                <div class="text-center alert alert-primary">
                                    In order to transfer the Cash reward Kindly upload the following documents (latest by <strong>31-Dec-2023</strong>)
                                </div>
                            }

                        </div>
                        <div class="col-12 col-md-12">
                            <div class="wizard-progress mb-2">
                                <div class="step @ViewBag.isicard">
                                    <a href="#0">
                                        Student <span class="tooltiptext">@(string.IsNullOrEmpty(ViewBag.isicard) ? "pending" : ViewBag.isicard)</span>
                                        <div class="node"></div>
                                    </a>
                                </div>
                                <div class="step @ViewBag.ispicard">
                                    <a href="#0">
                                        Parent <span class="tooltiptext">@(string.IsNullOrEmpty(ViewBag.ispicard) ? "pending" : ViewBag.ispicard)</span>
                                        <div class="node"></div>
                                    </a>
                                </div>
                                <div class="step @ViewBag.ismarksheet">
                                    <a href="#0">
                                        Mark sheet <span class="tooltiptext">@(string.IsNullOrEmpty(ViewBag.ismarksheet) ? "pending" : ViewBag.ismarksheet)t</span>
                                        <div class="node"></div>
                                    </a>
                                </div>
                                <div class="step @ViewBag.isbankaccount">
                                    <a href="#0">
                                        Cancelled Cheque or Passbook <span class="tooltiptext">@(string.IsNullOrEmpty(ViewBag.isbankaccount) ? "pending" : ViewBag.isbankaccount)</span>
                                        <div class="node"></div>
                                    </a>
                                </div>
                                <div class="step @ViewBag.isRTGS">
                                    <a href="#0">
                                        Bank details (RTGS) <span class="tooltiptext">@(string.IsNullOrEmpty(ViewBag.isRTGS) ? "pending" : ViewBag.isRTGS)</span>
                                        <div class="node"></div>
                                    </a>
                                </div>

                                @*  <div class="step in-progress complete">
                            Treatment
                            <div class="node"></div>
                            </div> *@
                            </div>
                        </div>
                    </div>

                    @* <div class="cd-horizontal-timeline loaded mt-0">
                <div class="timeline">
                <div class="events-wrapper">
                <div class="events">
                <ol>
                <li><a href="#0" data-date="01/01/2018" class="selected" style="left: 50px;">Student</a></li>
                <li><a href="#0" data-date="03/01/2018" style="left: 200px;">Parent</a></li>
                <li><a href="#0" data-date="05/01/2018" style="left: 480px;">Class</a></li>
                <li><a href="#0" data-date="07/01/2018" style="left: 600px;">Mark sheet</a></li>
                <li><a href="#0" data-date="10/01/2018" style="left: 780px;">Cancelled Cheque or Passbook</a></li>
                <li><a href="#0" data-date="18/01/2018" style="left: 960px;">Bank details (RTGS)</a></li>
                </ol>
                <span class="filling-line" aria-hidden="true" style="transform: scaleX(0.129016);"></span>
                </div>
                <!-- .events -->
                </div>
                <!-- .events-wrapper -->
                <ul class="cd-timeline-navigation">
                <li><a href="#0" class="prev inactive">Prev</a></li>
                <li><a href="#0" class="next">Next</a></li>
                </ul>
                <!-- .cd-timeline-navigation -->
                </div>
                </div> *@
                }
                @RenderBody()
            </div>
        </div>
    </div>


    <!-- ////////////////////////////////////////////////////////////////////////////-->


    <footer class="footer footer-static footer-transparent">
        <p class="clearfix blue-grey lighten-2 text-sm-center mb-0 px-2">
            <span class="float-md-left d-block d-md-inline-block">
                <i class="icon-shield"></i> Disclaimer: Though utmost care has been taken while preparing and uploading the TALLENTEX-@LNResources.Session result and other information, yet ALLEN Career Institute Pvt. Ltd. reserves all rights to rectify inadvertent errors, if any.
            </span>
        </p>
    </footer>

    <!-- BEGIN VENDOR JS-->
    <script src="~/app-assets/vendors/js/vendors.min.js" type="text/javascript"></script>
    <!-- BEGIN VENDOR JS-->
    <!-- BEGIN PAGE VENDOR JS-->
    <script src="~/app-assets/vendors/js/timeline/horizontal-timeline.js" type="text/javascript"></script>
    <!-- END PAGE VENDOR JS-->
    <!-- BEGIN MODERN JS-->
    <script src="~/app-assets/js/core/app-menu.js" type="text/javascript"></script>
    <script src="~/app-assets/js/core/app.js" type="text/javascript"></script>
    <!-- END MODERN JS-->
    <!-- BEGIN PAGE LEVEL JS-->

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <script src="~/js/logout.js"></script>
</body>
</html>
